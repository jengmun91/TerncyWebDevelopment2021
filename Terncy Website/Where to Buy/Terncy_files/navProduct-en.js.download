var shangla = `<div class="shangla-bg"></div>`
$('body').prepend(shangla);
$('.shangla-bg').hide();
$(function () {
    $(window).scroll(function () {
        if ($(window).scrollTop() > 50) {
            $(".shangla-bg").fadeIn(400);
        } else {
            $(".shangla-bg").fadeOut(400);
        }
    });
    $(".shangla-bg").click(function () {
        $('body,html').animate({
            scrollTop: 0
        }, 500);
        return false;
    })
})


